<h1 class="contactcount">There are <%= Contact.all.count %> contacts</h1>

<ul>
<% Contact.all.each do |contact| %>
<div class="contact">
  <li class="contact_info"><%= contact.first_name %> <%= contact.last_name %> (<%= contact.email%>)</li>
  <p><a href="/edit">Edit</a> | <a href="/contacts/delete">Delete</a></p>
</div>
<% end %>
</ul>
